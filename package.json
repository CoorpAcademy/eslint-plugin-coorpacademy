{
  "name": "@coorpacademy/eslint-plugin-coorpacademy",
  "version": "8.3.0",
  "description": "Common ESLint config for Coorpacademy projects",
  "main": "index.js",
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "eslint-ast-utils": "^1.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-ava": "^6.0.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-flowtype": "~3.4.2",
    "eslint-plugin-flowtype-errors": "^4.1.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-lodash-fp": "^2.1.3",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "~7.12.4",
    "eslint-plugin-unicorn": "^8.0.1",
    "lodash": "^4.17.11",
    "prettier": "1.16.4",
    "requireindex": "^1.2.0"
  },
  "peerDependencies": {
    "eslint": ">=5"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "eslint": "^4.18.2",
    "eslint-ava-rule-tester": "^3.0.0",
    "eslint-find-rules": "^3.3.1",
    "flow-bin": "^0.95.1",
    "nyc": "^13.3.0"
  },
  "scripts": {
    "test": "npm run lint && npm run test:unit && npm run find-missing",
    "test:unit": "nyc ava",
    "find-missing": "npm run find-missing:core && npm run find-missing:ava && npm run find-missing:lodash-fp && npm run find-missing:react && npm run find-missing:mocha && npm run find-missing:prettier && npm run find-missing:css-modules && npm run find-missing:flowtype",
    "find-missing:core": "eslint-find-rules -v --unused ./config/core.js",
    "find-missing:ava": "eslint-find-rules -v --unused --no-core ./config/ava.js",
    "find-missing:lodash-fp": "eslint-find-rules -v --unused --no-core ./config/lodash-fp.js",
    "find-missing:mocha": "eslint-find-rules -v --unused --no-core ./config/mocha.js",
    "find-missing:prettier": "eslint-find-rules -v --unused --no-core ./config/prettier.js",
    "find-missing:react": "eslint-find-rules -v --unused --no-core ./config/react.js",
    "find-missing:flowtype": "eslint-find-rules -v --unused --no-core ./config/flowtype.js",
    "find-missing:css-modules": "eslint-find-rules -v --unused --no-core ./config/css-modules.js",
    "prettier-core": "prettier --print-width 100 --single-quote --no-bracket-spacing config/**/*.js rules/**/*.js test/**/*.js *.js",
    "lint": "npm run prettier-core -s -- --list-different",
    "lint:fix": "npm run prettier-core -- --write"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "files": [
    "index.js",
    "config",
    "rules",
    "utils"
  ],
  "nyc": {
    "all": true,
    "cache": true,
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CoorpAcademy/eslint-plugin-coorpacademy.git"
  },
  "author": "Jeroen Engels <jfm.engels@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CoorpAcademy/eslint-plugin-coorpacademy/issues"
  },
  "homepage": "https://github.com/CoorpAcademy/eslint-plugin-coorpacademy#readme"
}
